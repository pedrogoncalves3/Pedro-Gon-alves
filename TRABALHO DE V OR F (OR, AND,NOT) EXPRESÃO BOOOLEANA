OR = OU E UM OU E OUTRO(SE TIVER UM VERDADEIRO SEMPRE VAI SER VERDADEIRO)
AND = SE TIVER UM FALSO VAI SER FALSO
NOT = INVERTE OS SINAIS(SE FOR V VAI INVERTER PARA F)
- - - - - - - - - -- - - -  -- -  - - - - - - - - -  - - - - - - - - - -  - - - - 
atividade = 1

A= TRUE
B= FALSE

R= (A AND B) OR NOT (A AND B)
R= (V AND F) OR NOT (V AND F)
R= (F) OR NOT (F)
R= F OR V
R= V
