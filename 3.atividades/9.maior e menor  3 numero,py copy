import os 
os.system("clear")

nu1 = int(input("digite um número: "))
nu2 = int(input("digite outro número: "))
nu3 = int(input("digite outro número: "))
print()
import os
os.system("clear")
maior = max (nu1 , nu2,nu3)
menor = min (nu1 , nu2,nu3)
print()
print ("os numeros informados são: ", {nu1},{nu2},{nu3})
